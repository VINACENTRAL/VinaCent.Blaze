@using VinaCent.Blaze.Web.Resources
@inject IWebResourceManager WebResourceManager
@{
    WebResourceManager.AddScript(ApplicationPath + "view-resources/Views/Shared/Components/TenantChange/Default.js");
}
<div class="text-center tenant-change-component">
    <span>
        @L(LKConstants.CurrentTenant):

        @if (Model.Tenant != null)
        {
            <span title="@Model.Tenant.Name"><strong>@Model.Tenant.TenancyName</strong></span>
        }
        else
        {
            <span>@L(LKConstants.NotSelected)</span>
        }

        (<a href="javascript:;" data-bs-toggle="modal" data-bs-target="#TenantChangeModal">@L(LKConstants.Change)</a>)
    </span>
</div>
